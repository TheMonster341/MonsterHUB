local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()


local MonsterHub = Rayfield:CreateWindow({
   Name = "MonsterHub",
   LoadingTitle = "MonsterHub",
   LoadingSubtitle = "by ! The Monster#0723",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = MonsterHub, -- Create a custom folder for your hub/game
      FileName = "MonsterHub"
   },
   Discord = {
      Enabled = true,
      Invite = "44cTkWSVwe", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
      RememberJoins = false -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "MonsterHub",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/44cTkWSVwe)",
      FileName = "MonsterKey",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "4h5k35kh43k5g356363gj3456k"
   }
})

--MainTab

local MainTab = MonsterHub:CreateTab("Main") -- Title

local LinksSection = MainTab:CreateSection("Links")

local YoutubeButton = MainTab:CreateButton({
   Name = "Youtube Link",
   Callback = function()
   setclipboard("https://www.youtube.com/channel/UCB4L5_QxNKa_i3qy4DYg7Mg")
   end,
})

