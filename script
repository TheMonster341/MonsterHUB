local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("HeckroHUB", "Ocean")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Roblox Profile", "Copy Roblox Profile Link", function()
	setclipboard("https://www.roblox.com/users/4137791381/profile")
end)


MainSection:NewButton("Discord", "Copy Discord Invite Link", function()
    setclipboard("https://discord.gg/44cTkWSVwe")
end)


MainSection:NewButton("Youtube", "Copy Youtube Link", function()
	setclipboard("https://www.youtube.com/channel/UCB4L5_QxNKa_i3qy4DYg7Mg")
end)


MainSection:NewButton("Twitter", "Copy Twitter Link", function()
	setclipboard("https://twitter.com/Heckro34")
end)


MainSection:NewKeybind("ToggleUI", "Invs And Vis HeckroHub ", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)


